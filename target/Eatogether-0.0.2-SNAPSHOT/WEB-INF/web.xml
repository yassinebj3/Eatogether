<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<servlet>
		<servlet-name>Inscription</servlet-name>
		<servlet-class>com.eatogether.Controller.Inscription</servlet-class>
	</servlet>
	<servlet>
	
		<servlet-name>Connexion</servlet-name>
		<servlet-class>com.eatogether.Controller.Connexion</servlet-class>
	</servlet>
	<servlet>
		
		<servlet-name>Recherche</servlet-name>
		<servlet-class>com.eatogether.Controller.Recherche</servlet-class>
	</servlet>
	<servlet>
		
		<servlet-name>Profil</servlet-name>
		<servlet-class>com.eatogether.Controller.Profil</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Inscription</servlet-name>
		<url-pattern>/Inscription</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Connexion</servlet-name>
		<url-pattern>/Connexion</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Recherche</servlet-name>
		<url-pattern>/Recherche</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Profil</servlet-name>
		<url-pattern>/Profil</url-pattern>
	</servlet-mapping>
</web-app>